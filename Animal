class Animal:
    def __init__(self, id_animal, raza, edad):
        self.id_animal = id_animal
        self.raza = raza
        self.edad = edad

    def obtenerTupla(self):
        return (self.id_animal, self.raza, self.edad)

    def __str__(self):
        return f"Animal(id={self.id_animal}, raza={self.raza}, edad={self.edad})"

